    {
      "name": "List all resources",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Parse JSON and extract variables at the top",
              "var jsonData = pm.response.json();",
              "let resourceName = jsonData.data[1].name;",
              "pm.environment.set(\"resourceName\", resourceName);",
              "",
              "// 1. STATUS CODE VALIDATION",
              "pm.test(\"Status code is 200 OK\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"Status code name is OK\", function () {",
              "    pm.expect(pm.response.status).to.eql(\"OK\");",
              "});",
              "",
              "// 2. RESPONSE TIME VALIDATION",
              "pm.test(\"Response time is under 2000ms\", function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(2000);",
              "});",
              "",
              "// 3. RESPONSE HEADERS VALIDATION",
              "pm.test(\"Content-Type header is present\", function () {",
              "    pm.response.to.have.header(\"Content-Type\");",
              "});",
              "",
              "pm.test(\"Content-Type header contains application/json\", function () {",
              "    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
              "});",
              "",
              "// 4. RESPONSE BODY STRUCTURE VALIDATION",
              "pm.test(\"Response body is valid JSON\", function () {",
              "    pm.response.to.be.json;",
              "});",
              "",
              "pm.test(\"Response has required top-level fields\", function () {",
              "    pm.expect(jsonData).to.have.property(\"page\");",
              "    pm.expect(jsonData).to.have.property(\"per_page\");",
              "    pm.expect(jsonData).to.have.property(\"total\");",
              "    pm.expect(jsonData).to.have.property(\"total_pages\");",
              "    pm.expect(jsonData).to.have.property(\"data\");",
              "});",
              "",
              "pm.test(\"Response has support object\", function () {",
              "    pm.expect(jsonData).to.have.property(\"support\");",
              "    pm.expect(jsonData.support).to.have.property(\"url\");",
              "    pm.expect(jsonData.support).to.have.property(\"text\");",
              "});",
              "",
              "// 5. DATA TYPE VALIDATION",
              "pm.test(\"Pagination fields have correct data types\", function () {",
              "    pm.expect(jsonData.page).to.be.a(\"number\");",
              "    pm.expect(jsonData.per_page).to.be.a(\"number\");",
              "    pm.expect(jsonData.total).to.be.a(\"number\");",
              "    pm.expect(jsonData.total_pages).to.be.a(\"number\");",
              "});",
              "",
              "pm.test(\"Data field is an array\", function () {",
              "    pm.expect(jsonData.data).to.be.an(\"array\");",
              "});",
              "",
              "pm.test(\"Data array is not empty\", function () {",
              "    pm.expect(jsonData.data.length).to.be.above(0);",
              "});",
              "",
              "pm.test(\"Each resource item has required fields\", function () {",
              "    jsonData.data.forEach(function (item) {",
              "        pm.expect(item).to.have.property(\"id\");",
              "        pm.expect(item).to.have.property(\"name\");",
              "        pm.expect(item).to.have.property(\"year\");",
              "        pm.expect(item).to.have.property(\"color\");",
              "        pm.expect(item).to.have.property(\"pantone_value\");",
              "    });",
              "});",
              "",
              "pm.test(\"Each resource item has correct data types\", function () {",
              "    jsonData.data.forEach(function (item) {",
              "        pm.expect(item.id).to.be.a(\"number\");",
              "        pm.expect(item.name).to.be.a(\"string\");",
              "        pm.expect(item.year).to.be.a(\"number\");",
              "        pm.expect(item.color).to.be.a(\"string\");",
              "        pm.expect(item.pantone_value).to.be.a(\"string\");",
              "    });",
              "});",
              "",
              "// 6. RESOURCE NAME VARIABLE VALIDATION",
              "pm.test(\"resourceName is a string\", function () {",
              "    pm.expect(resourceName).to.be.a(\"string\");",
              "});",
              "",
              "pm.test(\"resourceName is not empty\", function () {",
              "    pm.expect(resourceName).to.not.be.empty;",
              "});",
              "",
              "pm.test(\"resourceName matches data[1].name in response\", function () {",
              "    pm.expect(resourceName).to.eql(jsonData.data[1].name);",
              "});",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
